    "@tanstack/react-store": "^0.6.1",
    "axios": "^1.7.9",
    "i18n": "^0.15.1",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.2.0",
    "socket.io-client": "^4.8.1",
